[smc]
url=http://172.18.1.150:8082
apikey=EiGpKD4QxlLJ25dbBEp20001
#apiversion=5.10

[loggers]
keys=root

[handlers]
keys=stream_handler,file_handler

[formatters]
keys=stream,console

[logger_root]
level=INFO
handlers=stream_handler,file_handler

[handler_stream_handler]
class=StreamHandler
level=DEBUG
formatter=stream
args=(sys.stderr,)

[handler_file_handler]
class=handlers.RotatingFileHandler
level=DEBUG
formatter=stream
args=('smc.log','maxBytes=10','backupCount=2')

[formatter_stream]
format=%(asctime)s %(levelname)s: [%(name)s] %(message)s

[formatter_console]
format=%(message)s
